			<script type="text/javascript">
				jQuery(document).ready(function () {
					window.prettyPrint && prettyPrint();
					jQuery('#image-gallery').lightSlider({
						gallery: true,
						item: 1,
						loop: true,
						thumbItem: 9,
						slideMargin: 0,
						enableDrag: false,
						currentPagerPosition: 'left',
						onSliderLoad: function (el) {
							el.lightGallery({
								selector: '#image-gallery .lslide'
							});
						}
					});
				});
			</script>
			<!-- Lity Lightbox -->
