<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <!--     
    The following set the version information for each project in the solution, in addition to the 
    .nuspec file version(s)    
    -->
      <PropertyGroup>
        
        <VersionMajor>1</VersionMajor>
        <VersionMinor>8</VersionMinor>
        <VersionPatch>1</VersionPatch>
        <VersionSuffix>2</VersionSuffix>        
        <BuildConfiguration>Release</BuildConfiguration>
        <ReleaseAssemblyInformationVersion>$(VersionMajor).$(VersionMinor).$(VersionPatch)</ReleaseAssemblyInformationVersion>
        <DevAssemblyInformationVersion>$(VersionMajor).$(VersionMinor).$(VersionPatch).$(VersionSuffix)</DevAssemblyInformationVersion>
        <AssemblyInformationVersion>$(DevAssemblyInformationVersion)</AssemblyInformationVersion>


        <UmbracoVersionMajor>7</UmbracoVersionMajor>
        <UmbracoVersionMinor>2</UmbracoVersionMinor>
        <UmbracoVersionPatch>4</UmbracoVersionPatch>
        <UmbracoVersionSuffix>0</UmbracoVersionSuffix>
        <UmbracoVersion>$(UmbracoVersionMajor).$(UmbracoVersionMinor).$(UmbracoVersionPatch)</UmbracoVersion>
        
      </PropertyGroup>

</Project>